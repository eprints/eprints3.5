<?xml version="1.0" ?>
<cite:citation xmlns="http://www.w3.org/1999/xhtml" xmlns:cite="http://eprints.org/ep3/citation" xmlns:epc="http://eprints.org/ep3/control">
<span class="ep_document_citation">
  <epc:if test="formatdesc">
    <cite:linkhere><span class="document_format"><epc:print expr="formatdesc"/></span></cite:linkhere>
    <epc:if test="main"><br/><span class="document_filename"><print expr="main"/></span></epc:if>
  </epc:if>
  <epc:if test="!formatdesc">
    <epc:if test="main"><span class="document_format"><print expr="main"/></span></epc:if>
  </epc:if>
  <epc:if test="content"> - <span class="document_content"><epc:print expr="content" /></span></epc:if>
  <epc:if test="security != 'public'">
    <br /><span class="document_security">Restricted to <epc:print expr="security"/><epc:if test="date_embargo"> until <epc:print expr="date_embargo"/>.</epc:if></span>
  </epc:if>
  <epc:if test="license">
    <br /><span class="document_license">Available under License <epc:phrase ref="licenses_description_{license.as_string()}" />.</span>
  </epc:if>
</span>
</cite:citation>
